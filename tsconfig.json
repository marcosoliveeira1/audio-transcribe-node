{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [
      "vitest/globals"
    ]
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "**/*.d.ts"
  ]
}